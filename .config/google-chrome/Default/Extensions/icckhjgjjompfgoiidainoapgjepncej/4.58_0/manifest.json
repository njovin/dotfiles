      "pause": {
         "description": "Play/Pause",
         "global": true,
         "suggested_key": {
            "default": "MediaPlayPause"
         }
      },
      "previous": {
         "description": "Previous Song",
         "global": true,
         "suggested_key": {
            "default": "MediaPrevTrack"
         }
      },
      "stop": {
         "description": "Stop Audio",
         "global": true,
         "suggested_key": {
            "default": "MediaStop"
         }
      },
      "thumbsDown": {
         "description": "Thumbs Down Song (If supported)",
         "global": true
      },
      "thumbsUp": {
         "description": "Thumbs Up Song (If supported)",
         "global": true
      }
