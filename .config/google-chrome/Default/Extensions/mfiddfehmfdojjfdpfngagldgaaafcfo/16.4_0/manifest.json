{
   "app": {
      "background": {
         "scripts": [ "all.js" ]
      }
   },
   "description": "BrowserStack Local lets you test local servers & HTML, CSS and JS files in 700+ real desktop and mobile browsers, across 11 OSes.",
   "externally_connectable": {
      "matches": [ "https://*.bsstag.com/*", "https://*.browserstack.com/*" ]
   },
   "icons": {
      "128": "img/icon_128.png",
      "16": "img/icon_16.png",
      "48": "img/icon_48.png"
   },
   "key": "",
   "manifest_version": 2,
   "minimum_chrome_version": "31.0",
   "name": "BrowserStack Local",
   "permissions": [ {
      "socket": [ "tcp-connect", "tcp-listen" ]
   }, "http://*/", "https://*/", {
      "fileSystem": [ "write", "retainEntries", "directory" ]
   }, "clipboardWrite", "app.window", "storage" ],
   "update_url": "https://clients2.google.com/service/update2/crx",
   "version": "16.4"
}
